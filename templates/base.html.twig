<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% trans %}Welcome!{% endtrans %}{% endblock %}</title>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>

 

    <div class="main_content">&nbsp;</div>
    
    {% for message in app.flashes('error') %}
        <div class="alert alert-primary">
         {# <div class="alertx"><a type="button"><b>X</b></a></div> #}
            {{ message }}
        </div>
    {% endfor %}
    {% block body %}{% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
    </body>
</html>
