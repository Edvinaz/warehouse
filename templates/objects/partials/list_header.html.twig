<div class="section_menu">
    <div class="level1buttons">
        <a class="first" href="{{ path('objects') }}">{% trans %}Objects list{% endtrans %}</a><!--
        --><a href="">{% trans %}Write-off list{% endtrans %}</a><!--
        --><a href="{{ path('edit_object', {'id': 0}) }}">{% trans %}New object{% endtrans %}</a><!--
        --><a class="last" href="">{% trans %}New write-off{% endtrans %}</a>
    </div>
    {% if object is defined %}
    <div class="level2buttons">
        <a class="first" href="{{ path('edit_object', {'id': object.id}) }}">
            {% trans %}Edit{% endtrans %}
        </a>
        {% if not object.reserved or not object.invoices %}
        <a class="danger" href="{{ path('delete_object', {'id': object.id}) }}">
            {% trans %}Delete{% endtrans %}
        </a>
        {% endif %}
        {% if not object.hasContract %}
        <a href="{{ path('object_contract_edit', {'objectId': object.id}) }}">
            {% trans %}New contract{% endtrans %}
        </a>
        {% endif %}
        <a href="{{ path('object_invoice', {'id': object.id}) }}">
            {% trans %}New invoice{% endtrans %}
        </a ><!--
        {# --><a href="{{ path('print_reserved_materials', {'id': object.id}) }}">
            {% trans %}Print reserved materials{% endtrans %}
        </a><!-- #}
        {# --><a href="{{ path('object_time_card', {'id': object.id}) }}">
            {% trans %}Time-card{% endtrans %}
        </a><!-- #}
        --><a href="{{ path('recalculate_object_details', {'objectId': object.id}) }}">
            {% trans %}Recalculate{% endtrans %}
        </a><!--
        --><a class="last" href="{{ path('object_info', {'id': object.id}) }}">
            {% trans %}Back{% endtrans %}
        </a>
    </div>
    {% endif %}
    {% if object_list is defined %}
    <form class="search" method="GET">
        <input class="first last" name="search" placeholder="objekto paieÅ¡ka..." size="150" value="{{ search }}"/><!--
        --><button type="submit" hidden>{% trans %}Search{% endtrans %}</button>
    </form>
    {% endif %}
</div>


